{
  "name": "@definixswap/interface",
  "version": "2.1.2",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development react-scripts start",
    "start:prod": "NODE_ENV=production CI=false dotenv -e .env.production react-scripts start",
    "versionfile": "echo $npm_package_version > ./build/version.txt",
    "build": "react-scripts build",
    "build:dev": "NODE_ENV=development CI=false dotenv -e .env.development react-scripts build",
    "build:staging_prev": "NODE_ENV=staging CI=false dotenv -e .env.staging_prev react-scripts build",
    "build:stage": "NODE_ENV=production CI=false dotenv -e .env.stage react-scripts build",
    "postbuild:stage": "npm run versionfile",
    "build:production": "NODE_ENV=production CI=false dotenv -e .env.production react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lang": "gsheet-to-json ./lang.json ./src/locales",
    "integration-test": "start-server-and-test 'serve build -l 3000' http://localhost:3000 'cypress run'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "staging": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
